{
  "name": "sdek-api-lib",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript SDK for CDEK API v2.0. Complete delivery service integration with full type support, automatic token management, and extensive examples.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src/index.ts --out docs",
    "prepare": "npm run build",
    "example": "ts-node examples/live-example.ts",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:advanced": "ts-node examples/advanced-usage.ts"
  },
  "keywords": [
    "cdek",
    "api",
    "sdk",
    "typescript",
    "delivery",
    "shipping",
    "logistics",
    "russia",
    "courier",
    "pickup",
    "postamat",
    "ecommerce",
    "fulfillment"
  ],
  "author": "CDEK API SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barsbay/sdek-api-lib.git"
  },
  "bugs": {
    "url": "https://github.com/barsbay/sdek-api-lib/issues"
  },
  "homepage": "https://github.com/barsbay/sdek-api-lib#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "GUIDE.md",
    "LICENSE"
  ],
  "dependencies": {
    "dotenv": "^16.5.0",
    "node-fetch": "^2.7.0"
  }
}
